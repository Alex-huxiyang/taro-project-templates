    "@tarojs/cli": "{{ version }}",
    "babel-preset-taro": "{{ version }}",
    "eslint-config-taro": "{{ version }}",
    "eslint": "^8.12.0",
    "stylelint": "^14.4.0",
    "@types/webpack-env": "^1.13.6",
    "webpack": "5.91.0",
    "@tarojs/taro-loader": "{{ version }}",
    "@tarojs/webpack5-runner": "{{ version }}",{{#if (includes "React" "Preact" s=framework)}}
